{
  "version": 2,
  "builds": [
    { 
      "src": "build_files.sh",
      "use": "@vercel/static-build" 
    },
    {
      "src": "projeto_integrador/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "projeto_integrador/wsgi.py"
    }
  ]
}

